int k;
         Scanner inp = new Scanner(System.in);
         System.out.println("sinir sayisini giriniz : ");
         k = inp.nextInt();

         for ( int i=4 ; i<=k ; i*=4){
             System.out.println("girilen sayidan kucuk dordun kuvvetleri : "+ i);

                  }

         for (int i =5; i<=k; i*=5){
             System.out.println("girilen sayidan kucuk besin kuvvetleri : " + i);
         }

